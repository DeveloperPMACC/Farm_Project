{
    "name": "farm-project",
    "version": "1.0.0",
    "description": "Sistema automatizado para interacción con aplicaciones móviles utilizando Raspberry Pi y Node.js",
    "main": "app.js",
    "scripts": {
      "start": "node app.js",
      "dev": "nodemon app.js",
      "lint": "eslint .",
      "test": "jest",
      "db:create": "sequelize-cli db:create",
      "db:migrate": "sequelize-cli db:migrate",
      "db:seed": "sequelize-cli db:seed:all",
      "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
    },
    "dependencies": {
      "adbkit": "^2.11.1",
      "bcrypt": "^5.1.0",
      "compression": "^1.7.4",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.0",
      "helmet": "^6.1.5",
      "joi": "^17.9.2",
      "jsonwebtoken": "^9.0.0",
      "morgan": "^1.10.0",
      "pg": "^8.10.0",
      "pg-hstore": "^2.3.4",
      "sequelize": "^6.31.1",
      "socket.io": "^4.6.1",
      "uuid": "^9.0.0",
      "winston": "^3.8.2"
    },
    "devDependencies": {
      "eslint": "^8.39.0",
      "jest": "^29.5.0",
      "nodemon": "^2.0.22",
      "sequelize-cli": "^6.6.0",
      "supertest": "^6.3.3"
    }
  }